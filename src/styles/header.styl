#header-main
  height $headerHeight

  .site-branding
    display inline-block
    width 300px
    
  .site-logo
    float left
    margin-right $push
    img
      height $headerHeight
      width auto

  .custom-logo-link
    vertical-align middle
    display inline-block

  .site-title
    font-size 1em
    margin-top 0

.menu-main-container
  float right

  li
    float left
    margin-right $push
    list-style none